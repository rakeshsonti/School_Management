<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/j2ee/dtd/web-jsptaglibrary_1_2.dtd">

<!-- a tag library descriptor -->

<taglib>
  <tlib-version>1.0</tlib-version>
  <jsp-version>1.2</jsp-version>
  <short-name>debug</short-name>
  <uri>http://tomcat.apache.org/debug-taglib</uri>
  <description>
  Our Custom Tag    
  </description>
  <tag>
    <name>Iterator</name>
    <tag-class>com.thinking.machines.school.tags.IteratorTagHandler</tag-class>
    <description>
        To iterate a collection
    </description>
    <attribute>
        <name>collection</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
    <attribute>
        <name>name</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute> 
   </tag>
   <tag>
    <name>Equals</name>
    <tag-class>com.thinking.machines.school.tags.EqualsTagHandler</tag-class>
    <description>
 EQUALS TAG
    </description>
    <attribute>
        <name>leftOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
<attribute>
        <name>rightOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>       
</tag>



   <tag>
    <name>NotEquals</name>
    <tag-class>com.thinking.machines.school.tags.NotEqualsTagHandler</tag-class>
    <description>
      NotEQUALS TAG
    </description>
    <attribute>
        <name>leftOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
<attribute>
        <name>rightOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>       
</tag>

   <tag>
    <name>LessThan</name>
    <tag-class>com.thinking.machines.school.tags.LessThanTagHandler</tag-class>
    <description>
       less than tag implemented
    </description>
    <attribute>
        <name>leftOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
<attribute>
        <name>rightOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>       
</tag>



   <tag>
    <name>GreaterThan</name>
    <tag-class>com.thinking.machines.school.tags.GreaterThanTagHandler</tag-class>
    <description>
        greater than tag
    </description>
    <attribute>
        <name>leftOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
<attribute>
        <name>rightOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>       
</tag>


   <tag>
    <name>LessThanEquals</name>
    <tag-class>com.thinking.machines.school.tags.LessThanEqualsTagHandler</tag-class>
    <description>
        Less than equals tag implemented
    </description>
    <attribute>
        <name>leftOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
<attribute>
        <name>rightOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>       
</tag>
   <tag>
    <name>GreaterThanEquals</name>
    <tag-class>com.thinking.machines.school.tags.GreaterThanEqualsTagHandler</tag-class>
    <description>
        For compare collection that they are equal or not
    </description>
    <attribute>
        <name>leftOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
<attribute>
        <name>rightOperand</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>       
</tag>


   <tag>
    <name>Contains</name>
    <tag-class>com.thinking.machines.school.tags.ContainsTagHandler</tag-class>
    <description>
 Contains TAG
    </description>
    <attribute>
        <name>collection</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
<attribute>
        <name>item</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>       
</tag>

   <tag>
    <name>DoesNotContain</name>
    <tag-class>com.thinking.machines.school.tags.DoesNotContainTagHandler</tag-class>
    <description>
does not Contains TAG
    </description>
    <attribute>
        <name>collection</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>
<attribute>
        <name>item</name>
        <required>true</required>
	<rtexprvalue>true</rtexprvalue>    
    </attribute>       
</tag>


</taglib>
